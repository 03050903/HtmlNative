<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mozz.htmlnativedemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.mozz.htmlnativedemo.DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mozz.htmlnativedemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.mozz.htmlnativedemo.LayoutExampleActivity"
            android:label="Layout"/>

        <activity
            android:name="com.mozz.htmlnativedemo.ExampleListActivity"
            android:label="Example"/>

        <activity
            android:name=".WebViewActivity"
            android:label="Browser"/>

        <activity android:name=".TestActivity"/>

        <activity
            android:name=".SourceHtmlActivity"
            android:label="Html"/>
    </application>

</manifest>